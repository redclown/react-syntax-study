        <script type="text/javascript">
            $(function() {
                // 侧边栏二级菜单展开收起
                $('.opration-aside').on('click', '.operation-submenu-hd', function() {
                    $(this).parent().toggleClass('active');
                    checkQuickScrollBox();
                });

                // 鼠标移入一级菜单，二级出现
                $('.operation-menu-list li').on('mouseenter', function() {
                    var $this = $(this);
                    var index = $this.index();
                    $('.operation-submenu-list').eq(index).addClass('active').siblings().removeClass('active');
                    checkQuickScrollBox();
                });
                $('.opration-aside').on('mouseleave', function() {
                    var index = $('.operation-menu-list .active').index();
                    $('.operation-submenu-list').eq(index).addClass('active').siblings().removeClass('active');
                    checkQuickScrollBox();
                });

                // 二级菜单快速滚动到底部
                $('.quick-submenu-btn').on('click', function() {
                    var $subMenu = $('.operation-submenu-box');
                    var $subMenuActive = $('.operation-submenu .active');
                    var subMenuHeight = $subMenu.height();
                    var activeHeight = $subMenuActive.height();
                    $subMenu.animate({ scrollTop: activeHeight - subMenuHeight }, 100);
                    $('.quick-submenu-btn').hide();
                });
                $('.operation-submenu-box').on('scroll', function() {
                    var $subMenu = $('.operation-submenu-box');
                    var $subMenuActive = $('.operation-submenu .active');
                    var subMenuHeight = $subMenu.height();
                    var activeHeight = $subMenuActive.height();
                    if ($subMenu.scrollTop() >= activeHeight - subMenuHeight) {
                        $('.quick-submenu-btn').hide();
                    } else {
                        $('.quick-submenu-btn').css('display', 'flex');
                    }
                });

                // 判断二级菜单是否出现快捷滚动按钮
                function checkQuickScrollBox() {
                    var $subMenu = $('.operation-submenu-box');
                    var $subMenuActive = $('.operation-submenu .active');
                    var subMenuHeight = $subMenu.height();
                    var activeHeight = $subMenuActive.height();
                    if (subMenuHeight < activeHeight) {
                        $('.quick-submenu-btn').css('display', 'flex');
                    } else {
                        $('.quick-submenu-btn').hide();
                    }
                }
                checkQuickScrollBox();


