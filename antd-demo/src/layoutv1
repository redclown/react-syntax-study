                    }else{
                        var operaFlag = false;
                    }
                    function operationHeaderSearch(event) {
                        var event = window.event || event;

                        if (!$(".operation-header-v1 .header-search-input").is(":focus")) {
                            return false;
                        }

                        var val = $('.operation-header-v1 .header-search-input').val();

                        if (val.trim() != '') {
                            $(".operation-header-v1 .header-close-icon img").show();
                            var leng = $('.operation-header-v1 .header-search-box li').length;
                            if (event.keyCode == 13) {
                                if ($('.operation-header-v1 .header-search-box').find('li.actived').length > 0) {
                                    var $li = $('.operation-header-v1 .header-search-box').find('li.actived');
                                    if($($li).attr("data-type") == "order"){
                                        if($($li).find(".the-btns").attr('data-btn-type') == '1'){
                                            var documentNo = $($li).find(".the-title").text();
                                            var documentType = $($li).find(".the-title").attr("data-document-type");
                                            var platform = $($li).find(".the-title").attr("data-platform") == 'true'?1:0;
                                            var shopId = $($li).find(".the-title").attr("data-shop-id");
                                            var url = '/report/module/common-flow-chart?documentNo='+documentNo+'&documentType='+documentType+'&platform='+platform + '&shopId='+shopId;
                                            if(window.location.href.indexOf('/report/module/common-flow-chart') >= 0){
                                                window.open(url, "_self")
                                            }else {
                                                window.open(url)
                                            }
                                        }else{
                                            var url = $($li).find("a").attr('data-link');
                                            window.open(url)
                                        }
                                    }else{
                                        var url = $('.operation-header-v1 .header-search-box').find('li.actived').find("a").attr('href');
                                        window.open(url)
                                    }
                                    searchBlur();
                                    $('.operation-header-v1 .header-search-input').focus();
                                    return false;
                                }
                            }

                            if (event.keyCode == 40) {

                                i++;
                                if ($('.operation-header-v1 .header-search-box li').eq(i).hasClass("group-title")) {
                                    i += 1
                                }
                                if (i >= leng) {
                                    i = 1;
                                }
                                keyTopBottom(i);
                                return false;
                            } else if (event.keyCode == 38) {
                                i--;
                                if ($('.operation-header-v1 .header-search-box li').eq(i).hasClass("group-title")) {
                                    i -= 1
                                }
                                if (i <= 0) {
                                    i = leng - 1;
                                }
                                keyTopBottom(i);
                                return false;
                            } else if(event.keyCode == 9){
                                return false;
                            }

                            if (event.keyCode != 38 && event.keyCode != 40 && event.keyCode != 37 && event.keyCode != 39) {
                                $.get('/acl/menu/fuzzy-query-menu-and-help-centre-knowledge-by-name?name=' + encodeURIComponent(val.trim()), function (result) {
                                    var data = result.data;
                                    var menusHtml = '';
                                    var helpCenterHtml = '';
                                    var menuDtos = data.menuDtos?data.menuDtos:[];
                                    var helpCentreElasticSearchDtos = data.helpCentreElasticSearchDtos?data.helpCentreElasticSearchDtos:[];
                                    var documentSyncDtos = data.documentSyncDtos?data.documentSyncDtos:[]; // 单号
                                    if (data) {
                                        $('.operation-header-v1 .header-search-box').show();

                                        if(documentSyncDtos.length >= 3 && menuDtos.length >= 3 && helpCentreElasticSearchDtos.length >= 3){
                                            menuDtos = menuDtos.splice(0, 3);
                                            helpCentreElasticSearchDtos = helpCentreElasticSearchDtos.splice(0, 3);
                                            var documentLen = 10 - helpCentreElasticSearchDtos.length - menuDtos.length;
                                            documentSyncDtos = documentSyncDtos.splice(0, documentLen)
                                        }else if((menuDtos.length == 0 && documentSyncDtos.length >= 5 && helpCentreElasticSearchDtos.length >= 5)
                                            ||(helpCentreElasticSearchDtos.length == 0 && documentSyncDtos.length >= 5 && menuDtos.length >= 5)){
                                            menuDtos = menuDtos.splice(0, 5);
                                            helpCentreElasticSearchDtos = helpCentreElasticSearchDtos.splice(0, 5);
                                            var documentLen = menuDtos.length == 0 ? (10 - helpCentreElasticSearchDtos.length) : (10 - menuDtos.length);
                                            documentSyncDtos = documentSyncDtos.splice(0, documentLen);
                                        }else {
                                            var documentLen = menuDtos.length == 0 ? (10 - helpCentreElasticSearchDtos.length) : (10 - menuDtos.length);
                                            documentSyncDtos = documentSyncDtos.splice(0, documentLen);
                                        }

                                        var orderLiHtml = '';
                                        if(documentSyncDtos && documentSyncDtos.length > 0){
                                            orderLiHtml = '<li class="group-title document-title">单号</li>';
                                            $.each(documentSyncDtos, function (i, e) {
                                                if(operaFlag && e.shopId != 9){
                                                    var detailUrl = '';
                                                }else if(!operaFlag && e.shopId == 9){
                                                    var detailUrl = '';
                                                }else{
                                                    if(e.documentType != 1 && e.documentType != 2 && !e.documentId){
                                                        var detailUrl = '';
                                                    }else {
                                                        if (e.documentType == 10) {
                                                            var detailUrl = getOrderDetailUrl(e.platform, e.documentType, e.documentNo, e.documentSignId);
                                                        } else {
                                                            var detailUrl = getOrderDetailUrl(e.platform, e.documentType, e.documentNo, e.documentId);
                                                        }
                                                    }
                                                }
                                                orderLiHtml += '<li data-type="order"><a href="javaScript:void(0);" data-link="' + detailUrl + '">' +
                                                    '<span class="the-title" data-id="'+ e.documentId +'" data-document-type="'+ e.documentType +'" data-shop-id="'+ e.shopId +'" data-platform="'+ e.platform +'">' + e.documentNo+'</span>' +
                                                    '<span class="the-product">'+ e.documentTypeText +'</span>' +
                                                    '<span class="the-btns" data-btn-type="1">' +
                                                    '   <button class="layui-btn layui-btn-small flow-btn active" data-btn-type="1">查流程</button>' +
                                                    (detailUrl == "" ? '' : '   <button class="layui-btn layui-btn-small detail-btn" data-btn-type="2">查详情</button>') +
                                                    '</span>' +
                                                    '</a></li>';
                                            })
                                        }

                                        var menusLiHtml = '';
                                        if (menuDtos.length > 0) {
                                            menusHtml = '<li class="group-title menus-title">菜单入口</li>';
                                            $.each(menuDtos, function (i, e) {
                                                menusLiHtml += '<li><a href="' + e.url + '"   target="_blank"><span class="the-title">' + e.name + '</span><span class="the-product">' + e.parentName + '</span></a></li>'
                                            });
                                            menusHtml += menusLiHtml;

                                        }
                                        var helpCenterLiHtml = '';
                                        if (helpCentreElasticSearchDtos.length > 0) {
                                            helpCenterHtml = '<li class="group-title helpCenter-title">帮助中心</li>';
                                            $.each(helpCentreElasticSearchDtos, function (i, e) {
                                                var url;
                                                var type = e.type;
                                                if (type == 1) {
                                                    //    常见问题
                                                    url = '/help/help/help-issue-detail?id=' + e.issueDetailId + '&categoryId=' + e.secondCategoryId
                                                } else {
                                                    //    操作手册
                                                    url = '/help/help/help-operation-index?categoryId=' + e.secondCategoryId + '&type=2'
                                                }
                                                helpCenterLiHtml += '<li><a href="' + url + '"  target="_blank"><span class="the-title">' + e.problemName + '</span><span class="the-product">' + e.firstName + '</span></a></li>'
                                            });
                                            helpCenterHtml += helpCenterLiHtml
                                        }
                                        $('.operation-header-v1 .search-box-list').html(menusHtml + helpCenterHtml + orderLiHtml);
                                    }
                                    if (menuDtos.length == 0 && helpCentreElasticSearchDtos.length == 0 && documentSyncDtos.length == 0) {
                                        $('.operation-header-v1 .search-box-list').html('<div class="group-title">没有找到相关内容</div>');
                                    }
                                });
                            }
                        } else {
                            i = 0;
                            $('.operation-header-v1 .header-search-box').hide();
                            $(".operation-header-v1 .header-close-icon img").hide();
                        }
                        $(document).off('keydown').keydown(function (event) {
                            if(event.keyCode == 9){
                                event.preventDefault();
                            }
                        })
                    }

                    function keyTopBottom(i) {
                        $('.operation-header-v1 .header-search-box li').eq(i).addClass('actived').siblings().removeClass('actived');
                        $('.operation-header-v1 .header-search-input').val($('.operation-header-v1 .header-search-box li').eq(i).find(".the-title").text());
                    }

                    function searchBlur() {
                        i = 0;
                        $('.operation-header-v1 .header-search-box').hide();
                        $('.operation-header-v1 .header-search-box ul').html('');
                    }

                    $('.operation-header-v1 .header-search-input').blur(function (event) {
                        $('.operation-header-v1 .header-search-box').undelegate('li', 'click').delegate('li', 'click', function () {
                            $('.operation-header-v1 .header-search-input').val($(this).find(".the-title").text());
                            if($(this).attr("data-type") == "order"){
                                if($(this).find(".the-btns").attr('data-btn-type') == '1'){
                                    var documentNo = $(this).find(".the-title").text();
                                    var documentType = $(this).find(".the-title").attr("data-document-type");
                                    var platform = $(this).find(".the-title").attr("data-platform") == 'true'?1:0;
                                    var shopId = $(this).find(".the-title").attr("data-shop-id");
                                    var url = '/report/module/common-flow-chart?documentNo='+documentNo+'&documentType='+documentType+'&platform='+platform+'&shopId='+shopId;
                                    window.open(url);
                                }else{
                                    var url = $(this).find("a").attr('data-link');
                                    window.open(url)
                                }
                            }
                            searchBlur();
                            $('.operation-header-v1 .header-search-input').focus();
                            event.preventDefault();
                        });

                        $(document).click(function () {
                            searchBlur();
                            event.preventDefault();
                        });
                    });
                    $('.operation-header-v1 .header-search-input').bind({
                        keyup: operationHeaderSearch,
                        click: operationHeaderSearch
                    });
                    $(".operation-header-v1 .header-close-icon").on("click", function () {
                        $(".header-search-input").val("");
                        $(".header-search-input").focus();
                        $(".operation-header-v1 .header-close-icon img").hide();
                    });
                    $(".operation-header-v1 .header-close-icon").hover(function () {
                        $(this).find("img").attr("src", "/static/images/operation-header-close2.png")
                    }, function () {
                        $(this).find("img").attr("src", "/static/images/operation-header-close1.png")
                    })

                    // tab键切换 查流程/查详情 按钮
                    $('.operation-header-v1 .header-search-input').on('keydown', function (ev) {
                        if(ev.keyCode == 9){
                            if($('.operation-header-v1 .header-search-box').find('li.actived').attr('data-type') == 'order'){
                                var $li = $('.operation-header-v1 .header-search-box').find('li.actived');
                                var btnType = $($li).find('.the-btns').attr('data-btn-type');
                                if(btnType == 1){
                                    $($li).find('.the-btns .detail-btn').addClass('active').siblings().removeClass('active');
                                    $($li).find('.the-btns').attr('data-btn-type', 2)
                                }else{
                                    $($li).find('.the-btns .flow-btn').addClass('active').siblings().removeClass('active');
                                    $($li).find('.the-btns').attr('data-btn-type', 1)
                                }
                                ev.preventDefault();
                            }
                        }
                    });
                    $('body').delegate('.operation-header-v1 .header-search-box .the-btns button', 'mouseover', function (ev) {
                        var btnType = $(this).attr('data-btn-type');
                        $(this).addClass("active").siblings().removeClass("active");
                        $(this).parent(".the-btns").attr('data-btn-type', btnType);
                        ev.stopPropagation();
                    });
                    // 单据号详情链接
                    function getOrderDetailUrl(platform, type, val, id) {
                        var blankUrl = '';
                        switch (type) {
                            case 1: // 订单号
                                if(platform){
                                    blankUrl = '/porder/operation/show-details?orderNo='+val
                                }else{
                                    blankUrl = '/order/operation/show-details?orderNo='+ val +'&type=2'
                                }
                                break;
                            case 2: // 售后单号
                                if(platform){
                                    blankUrl = '/p-after-sales/model-return/model-return-detail?afterSalesNum='+val;
                                }else{
                                    blankUrl = '/after-sales/model-return/model-return-detail?afterSalesNum='+val
                                }
                                break;
                            case 3: // 付款单号
                                if(platform){
                                    blankUrl = '/pfinance/purchase-payment/purchase-payment-detail-financial?id='+ id
                                }else{
                                    blankUrl = '/finance/purchase-payment/v3/purchase-payment-detail-index?id='+ id
                                }
                                break;
                            case 4: // 支付单号
                                break;
                            case 5: // 请购单号
                                if(platform){
                                    blankUrl = '/ppurchase/purchase-apply/detail-index-new?applyNo='+val
                                }else{
                                    blankUrl = '/purchase/purchase-apply/v3/index?applyNo='+val
                                }
                                break;
                            case 6: // 采购单号
                                if(platform) {
                                    blankUrl = '/ppurchase/purchase/show-purchase-detail?purchaseId=' + id;
                                }else{
                                    blankUrl = '/purchase/purchase/show-purchase-detail?purchaseId='+ id;
                                }
                                break;
                            case 7: // 采购退货单号
                                if(platform) {
                                    blankUrl = '/ppurchase/purchase-revert/show-purchase-revert-info?purchaseRevertId=' + id
                                }else{
                                    blankUrl = '/purchase/purchase-revert/show-purchase-revert-info?purchaseRevertId=' + id
                                }
                                break;
                            case 8: // 入库单号
                                blankUrl = '/inventory/stock-in/pre-detail-stock-in?id='+ id +'&type=50';
                                break;
                            case 9: // 收票单号 单号+状态+制单人+创建时间
                                if(platform){
                                    blankUrl = '/pfinance/invoice-in/invoice-in-detail-financial?isNew=true&id='+ id
                                }else{
                                    blankUrl = '/finance/invoice-in/v3/view-invoice-in-detail?id='+ id
                                }
                                break;
                            case 10: // 出库单号
                                // blankUrl = '/order/operation/show-details?orderNo=20190920223324'
                                blankUrl = '/inventory/stock-out/pre-detail-stock-out?signId=' + id;
                                break;
                            case 11: // 开票单号
                                if(platform){
                                    blankUrl = '/pfinance/invoice-out/search-invoice-out-financial?isNew=true&id='+ id
                                }else{
                                    blankUrl = '/finance/invoice-out/v3/view-invoice-out-detail?id='+ id
                                }
                                break;
                            case 12: // 寄售单号
                                if(platform) {
                                    blankUrl = '/consignment/operation/show-detail?delegateNo=' + val;
                                }else{
                                    blankUrl = '/consignment/delegate-detail/show-detail?delegateNo=' + val;
                                }
                                break;
                            case 13: // 结算单号
                                blankUrl = '/consignment/settlement-detail/show-detail?settlementId='+ id;
                                break;
                            case 14: // 快递单号
                                break;
                            case 15: // 供应商合同号
                                break;
                            case 16: // 汇票单号
                                break;
                            case 17: // 银行卡号
                                break;
                            case 18: // 自定义合同号
                                break;
                            case 19: // 发票号
                                break;
                            case 0: // 未定义
                                break;
                        }
                        return blankUrl;
                    }

                    //右侧图标
                    $('.help-box-hd').click(function () {
                        var pathName = window.location.pathname;
                        $.get('/help/help/redirect-operation-manual?url=' + pathName, function (result) {
                            if (result.data != null) {
                                var helpAlertBox = '<div class="help-alert-box">' +
                                    '<em>操作简介：</em>' +
                                    '<p>' + result.data.summary + '</p>' +
                                    '<div class="more"><a href="/help/help/help-operation-index?url=' + pathName + '" target="_black">查看更多</a></div>' +
                                    '</div>';

                                layui.layer.open({
                                    title: '帮助',
                                    type: 1,
                                    content: helpAlertBox
                                });
                            }
                        });
                    });
                    $('.operation-header-v1 .login-quit').click(function () {
                        var ticket = gks.getCookie('ticket');
                        $.post('/user/session/login-out', {'ticket': ticket}, function (data) {
                            if (data.code == 0) {
                                var exp = new Date();
                                exp.setTime(exp.getTime() - 1);
                                var cval = gks.getCookie('ticket');
                                if (cval != null) {
                                    document.cookie = "ticket=" + cval + ";domain=.gongkongsaas.com;path=/;expires=" + exp.toGMTString();
                                }
                                if (window.location.href.indexOf('gongkongsaas') < 0 || window.location.href.indexOf('b.gongkongsaas') >= 0) {
                                    window.location.href = '/user/operation/pre-login';
                                } else {
                                    window.location.href = '/user/employee/pre-login';
                                }

                            }
                        });
                    });
                    $(".operation-header-v1 .check-password").click(function () {
                        layer.open({
                            title: '修改密码',
                            type: 1,
                            content: '<div class="unbind-reset-pwd" style="margin-top: 25px;">' +
                                '<form class="layui-form" lay-filter="myFrom" id="myFrom"><input type="hidden" id="id" name="id" value=""/>' +
                                '<div class="layui-form-item add-mobile"><div class="layui-form-item">' +
                                '<label class="layui-form-label">原密码<span style="color:red;padding-left: 3px;">*</span></label><div class="layui-input-inline">' +
                                '<input type="password" name="unbindOldPassword" id="unbindOldPassword" autocomplete="off" class="layui-input"></div></div>' +
                                '<div class="layui-form-item"><label class="layui-form-label">新密码<span style="color:red;padding-left: 3px;">*</span></label>' +
                                '<div class="layui-input-inline">' +
                                '<input type="password" name="unbindNewPassword" id="unbindNewPassword" autocomplete="off" class="layui-input"></div></div></div></form>' +
                                '</div>',
                            area: ['300px', '230px'],
                            shade: 0,
                            btn: ['保存', '取消'],
                            success: function () {
                                /**
                                 *   功能：设置input的验证规则
                                 *   参数：
                                 *      config：{object}
                                 *          el：元素
                                 *          triggerCheck：{object}
                                 *              'onblur'：[{'functionName': string, errorDescripion: string },....]
                                 *
                                 *
                                 * */
                                function checkInput(config) {

                                    if (!config.el) {
                                        return;
                                    }

                                    var triggerCheck = config.triggerCheck;
                                    if (typeof triggerCheck === 'object') {

                                        for (var i in triggerCheck) {

                                            $(config.el).on(i, function () {
                                                var that = this,
                                                    checks = triggerCheck[i].length;
                                                for (var j = 0; j < checks; j++) {
                                                    var functionName = triggerCheck[i][j].functionName,
                                                        errorDescripion = triggerCheck[i][j].errorDescripion;
                                                    if (functionName(that)) {
                                                        //  验证通过，则继续
                                                        continue;
                                                    } else {
                                                        // 提示错误信息
                                                        layer.msg(errorDescripion, {icon: 0});
                                                        break;
                                                    }
                                                }
                                            });

                                        }

                                    }
                                }

                                var checkRule = {
                                    checkBlank: function (el) {
                                        var val = $(el).val()
                                        valLength = val.length;
                                        if (valLength !== 0) {
                                            if (val[valLength - 1].includes(' ')) {
                                                $(el).val(val.trim());
                                                return false;
                                            }
                                        }
                                        return true;
                                    },

                                };
                                checkInput({
                                    el: '#unbindOldPassword,#unbindNewPassword',
                                    triggerCheck: {
                                        'input': [{
                                            functionName: checkRule.checkBlank,
                                            errorDescripion: '不能输入空格等空白字符'
                                        }],
                                    }
                                });
                            },
                            yes: function (index,layero) {
                                console.log(layero)
                                var oldPasswordInput=layero.find("#unbindOldPassword");
                                var newPasswordInput=layero.find("#unbindNewPassword");
                                var oldPassword = oldPasswordInput.val();
                                if (oldPassword == null || oldPassword == undefined || oldPassword == '') {
                                    layer.msg("原密码不能为空", {icon: 0});
                                    return;
                                }
                                var newPassword =newPasswordInput.val();
                                // if (newPassword == null || newPassword == undefined || newPassword == '') {
                                //     layer.msg("新密码不能为空",{icon: 0});
                                //     return;
                                // }
                                // if (newPassword.length < 6 || newPassword.length > 18) {
                                //     layer.msg("长度必须为6-18位",{icon: 0});
                                //     return;
                                // }
                                if (oldPassword == newPassword) {
                                    layer.msg("新密码不能和原密码相同", {icon: 0});
                                    return;
                                }
                                var loginName = $('.o-username input').attr('data-con');
                                if (!checkPassword(newPassword, loginName)) {
                                    layer.msg("请输入6-18位字符的密码！", {icon: 0});
                                    return;
                                }

                                function checkPassword(password, loginName) {

                                    if (!password || password.indexOf(" ") >= 0 || password.length < 6 || password.length > 18) {
                                        return false;
                                    }

                                    /*if(password[0].charCodeAt() > password[1].charCodeAt()){
                                        for (var i = 1; i < password.length; i++)
                                        {
                                            if (parseInt(password[i - 1].charCodeAt()) != parseInt(password[i].charCodeAt()) + parseInt(1)){
                                                return false;
                                            }
                                        }
                                    }else {
                                        for (var i = 1; i < password.length; i++){
                                            if (parseInt(password[i].charCodeAt()) != parseInt(password[i - 1].charCodeAt()) + parseInt(1)) {
                                                return false;
                                            }
                                        }
                                    }*/
                                    return true;
                                }
                                $.ajax({
                                    type: 'POST',
                                    url: '/user/shop-account/reset-password',
                                    data: {
                                        "oldPassword": oldPassword,
                                        "newPassword": newPassword
                                    },
                                    dataType: 'JSON',
                                    success: function (data) {
                                        if (data.code == 0) {
                                            layer.msg("修改成功! 请重新登录!",{icon: 1},function () {
                                                window.parent.location.reload();
                                            });
                                            layer.close(index);
                                        }
                                        else {
                                            layer.msg(data.message,{icon: 0});
                                        }
                                    },
                                    error: function () {
                                        console.log("发生异常");
                                    }
                                });
                            }
                        });
                    });
                    $('.operation-header-v1 .name').mouseenter(function () {
                        var $this = $(this);
                        var qrCodeUrl = "/user/we-chat/scan-qr-code?callBackReturnUrl=" + window.location.pathname;
                        if ($this.attr('data-mouse')) {
                            // 如果已经请求一次不需要再请求了
                        } else {
                            $.ajax({
                                type: "post",
                                url: "/user/shop-account/get-user-info",
                                dataType: "json",
                                success: function (result) {
                                    if (result.code == 0) {
                                        if (result.data.weChatLoginOpen) {
                                            // 如果绑定了微信
                                            if (result.data.bindWeChat == true) {
                                                $('.o-wechat a').text('解绑');
                                                $('.o-wechat a').attr('href', '');
                                                $('.o-wechat a').on('click', function () {
                                                    layer.confirm('你确定要解绑吗？', {btn: ['确定', '取消']}, function (index) {
                                                        $.ajax({
                                                            type: 'POST',
                                                            url: '/user/we-chat/cancel-binding',
                                                            dataType: 'JSON',
                                                            success: function (data) {
                                                                if (data.code == 0) {
                                                                    layer.msg("解绑成功");
                                                                    layer.close(index);
                                                                    window.parent.location.reload();
                                                                }
                                                                else {
                                                                    layer.msg(data.message);
                                                                }
                                                            },
                                                            error: function () {
                                                                layer.msg("发生异常");
                                                            }
                                                        });
                                                    });
                                                    return false;
                                                });
                                            } else {
                                                $('.o-wechat a').text('绑定');
                                                $('.o-wechat a').attr('href', qrCodeUrl);
                                            }
                                        } else {
                                            $('.o-wechat').remove();
                                        }
                                        // 保存用户类型
                                        gks.userType = result.data.type;

                                        $('.o-username input').val(result.data.loginname);
                                        $('.o-username input').attr('data-con', result.data.loginname);
                                        $('.o-realname input').val(result.data.realname);
                                        $('.o-realname input').attr('data-con', result.data.realname);
                                        $('.o-othername input').val(result.data.nickname);
                                        $('.o-othername input').attr('data-con', result.data.nickname);
                                        $('.o-party input').val(result.data.departmentName);
                                        $('.o-party input').attr('data-con', result.data.departmentName);
                                        if (result.data.roleDtoList != null) {
                                            $('.o-role input').val(result.data.roleDtoList[0].name);
                                            $('.o-role input').attr('data-con', result.data.roleDtoList[0].name);
                                            $('.o-role input').attr('title', result.data.roleDtoList[0].rangeTypeName);
                                        }
                                        if (result.data.referralCode) {
                                            $('.o-referralCode input').val(result.data.referralCode);
                                            $('.o-referralCode').show();
                                        } else {
                                            $('.o-referralCode').hide();
                                        }
                                        $('.o-code input').val(result.data.referralCode);
                                        $('.o-phone input').val(result.data.mobile);
                                        $('.o-phone input').attr('data-con', result.data.mobile);
                                        $('.o-email input').val(result.data.email);
                                        $('.o-email input').attr('data-con', result.data.email);
                                        $('.o-qq input').val(result.data.qq);
                                        $('.o-qq input').attr('data-con', result.data.qq);
                                        $('.login-out .fl').html('上次登录：' + result.data.lastLoginTime.replace("T", " "));

                                        $this.attr('data-mouse', true);
                                    } else {
                                        console.log("操作失败！");
                                    }
                                },
                                error: function (XMLHttpRequest, textStatus, errorThrown) {
                                    console.log("操作失败！");
                                }
                            });
                        }

                        $('.operation-header-v1 .name-box').show();
                        $('.operation-header-v1 .check').click(function (event) {
                            $(this).prev('.con').find('input').prop('disabled', false).css('border', '1px #e5e5e5 solid').focus();
                            $(this).prev('.con').find('.layui-btn-group').show();
                            $(this).hide();
                            // event.preventDefault();
                        });
                    });
                    $('.operation-header-v1 .name').mouseleave(function () {
                        $('.operation-header-v1 .name-box').hide();
                    });
                    $(".operation-header").delegate('.o-realname .layui-btn-group span:eq(0)', "click", function () {
                        console.log("修改名字")
                        var $this = $(this);
                        var ajaxVal = $(this).parents('.con').find('input').val();
                        var userDto = {};
                        userDto.realname = ajaxVal;
                        $.ajax({
                            type: "POST",
                            url: "/user/shop-account/update-account-info",
                            data: JSON.stringify(userDto),
                            dataType: "json",
                            contentType: 'application/json;charset=utf-8',
                            success: function (result) {
                                if (result.code == 0) {
                                    $this.parent().hide();
                                    $this.parent().prev().attr('data-con', $this.parent().prev().val()).prop('disabled', true).css('border', '1px #fff solid');
                                    $this.parents('.con').next().show();
                                    layer.msg("修改成功！");

                                } else {
                                    layer.msg(result.message);
                                }
                            },
                            error: function (XMLHttpRequest, textStatus, errorThrown) {
                                console.log("修改失败！");
                            }
                        });

                    })

                    $('.operation-header-v1 .o-othername .layui-btn-group span').eq(0).click(function (event) {
                        var $this = $(this);
                        var ajaxVal = $(this).parents('.con').find('input').val();
                        var userDto = {};
                        userDto.nickname = ajaxVal;
                        $.ajax({
                            type: "POST",
                            url: "/user/shop-account/update-account-info",
                            data: JSON.stringify(userDto),
                            dataType: "json",
                            contentType: 'application/json;charset=utf-8',
                            success: function (result) {
                                if (result.code == 0) {
                                    $this.parent().hide();
                                    $this.parent().prev().attr('data-con', $this.parent().prev().val()).prop('disabled', true).css('border', '1px #fff solid');
                                    $this.parents('.con').next().show();
                                    layer.msg("修改成功！");
                                } else {
                                    layer.msg("修改失败！");
                                }
                            },
                            error: function (XMLHttpRequest, textStatus, errorThrown) {
                                console.log("修改失败！");
                            }
                        });
                    });

                    $('.operation-header-v1 .o-phone .layui-btn-group span').eq(0).click(function (event) {
                        var $this = $(this);
                        var ajaxVal = $(this).parents('.con').find('input').val();
                        var userDto = {};
                        userDto.mobile = ajaxVal;
                        $.ajax({
                            type: "POST",
                            url: "/user/shop-account/update-account-info",
                            data: JSON.stringify(userDto),
                            dataType: "json",
                            contentType: 'application/json;charset=utf-8',
                            success: function (result) {
                                if (result.code == 0) {
                                    $this.parent().hide();
                                    $this.parent().prev().attr('data-con', $this.parent().prev().val()).prop('disabled', true).css('border', '1px #fff solid');
                                    $this.parents('.con').next().show();
                                    layer.msg("修改成功！");
                                } else {
                                    layer.msg("修改失败！");
                                }
                            },
                            error: function (XMLHttpRequest, textStatus, errorThrown) {
                                console.log("修改失败！");
                            }
                        });
                    });

                    $('.operation-header-v1 .o-qq .layui-btn-group span').eq(0).click(function (event) {
                        var $this = $(this);
                        var ajaxVal = $(this).parents('.con').find('input').val();
                        var r = /^\+?[1-9][0-9]*$/;
                        if (!r.test(ajaxVal)) {
                            layer.tips('请输入数字格式！', '.o-qq', {
                                tips: [1, '#0FA6D8'] //还可配置颜色
                            });
                            return;
                        }
                        var userDto = {};
                        userDto.qq = ajaxVal;
                        $.ajax({
                            type: "POST",
                            url: "/user/shop-account/update-account-info",
                            data: JSON.stringify(userDto),
                            dataType: "json",
                            contentType: 'application/json;charset=utf-8',
                            success: function (result) {
                                if (result.code == 0) {
                                    $this.parent().hide();
                                    $this.parent().prev().attr('data-con', $this.parent().prev().val()).prop('disabled', true).css('border', '1px #fff solid');
                                    $this.parents('.con').next().show();
                                    layer.msg("修改成功！");
                                } else {
                                    layer.msg("修改失败！");
                                }
                            },
                            error: function (XMLHttpRequest, textStatus, errorThrown) {
                                console.log("修改失败！");
                            }
                        });
                    });

                    $('.operation-header-v1 .o-email .layui-btn-group span').eq(0).click(function (event) {
                        var $this = $(this);
                        var ajaxVal = $(this).parents('.con').find('input').val();
                        var userDto = {};
                        userDto.email = ajaxVal;
                        $.ajax({
                            type: "POST",
                            url: "/user/shop-account/update-account-info",
                            data: JSON.stringify(userDto),
                            dataType: "json",
                            contentType: 'application/json;charset=utf-8',
                            success: function (result) {
                                if (result.code == 0) {
                                    $this.parent().hide();
                                    $this.parent().prev().attr('data-con', $this.parent().prev().val()).prop('disabled', true).css('border', '1px #fff solid');
                                    $this.parents('.con').next().show();
                                    layer.msg("修改成功！");
                                } else {
                                    layer.msg("修改失败！");
                                }
                            },
                            error: function (XMLHttpRequest, textStatus, errorThrown) {
                                console.log("修改失败！");
                            }
                        });
                    });

                    $('.operation-header-v1 .con .o-close').click(function (event) {
                        var oldVal = $(this).parent().prev().attr('data-con');
                        $(this).parent().prev().val(oldVal).prop('disabled', true).css('border', '1px #fff solid');
                        $(this).parent().hide();
                        $(this).parent().parent().next().show();
                    });
                };

                gks.appendOperationHeaderOptionV1();
            });
        </script>
        <#include "header-v1.ftl">
